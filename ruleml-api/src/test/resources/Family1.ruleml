Document (
	Group (
		<!-- Deobjectified version -->
		Group (
			Forall ?Hu ?Wi ?Ch (
				family(husb->?Hu wife->?Wi child->?Ch):-
					And(married(?Hu ?Wi) Or(kid(?Hu ?Ch) kid(?Wi ?Ch)))
			)
			married(Joe Sue)
			kid(Sue Pete)
		)

		<!-- Objectified version -->
		Group (
			Forall ?Hu ?Wi ?Ch ?2 ?3 ?4 (
			Exists ?1 (
				?1#family(husb->?Hu wife->?Wi child->?Ch)) :-
						And(?2#married(?Hu ?Wi)	Or(?3#kid(?Hu ?Ch) ?4#kid(?Wi ?Ch)))
			)
			_1#married(Joe Sue)
			_2#kid(Sue Pete)
		)
	)
)